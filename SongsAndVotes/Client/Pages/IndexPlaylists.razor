@page "/playlists"

<div>
    <PlaylistsList Playlists="Playlists" />
</div>

@code {
    private List<Playlist> Playlists = new List<Playlist>();

    protected override void OnInitialized()
    {
        User u1 = new User();
        u1.Name = "Kopecek";

        User u2 = new User();
        u2.Name = "Pišta";


        Playlist playlist1 = new Playlist();
        playlist1.Name = "Best playlist";
        playlist1.Photo = "data:image/jpeg;base64,";
        playlist1.UserBy = u1;

        Playlist playlist2 = new Playlist();
        playlist2.Name = "Worst playlist";
        playlist2.Photo = "data:image/jpeg;base64,";

        playlist2.UserBy = u2;

        Playlists.Add(playlist1);
        Playlists.Add(playlist2);
    }
}
