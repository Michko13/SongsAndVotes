@page "/"

<div>
    <SongsList Songs="Songs" />
</div>

<div>
    <ArtistsList Artists="Artists" />
</div>

<div>
    <PlaylistsList Playlists="Playlists" />
</div>

@code{
    private List<Song> Songs;
    private List<Artist> Artists;
    private List<Playlist> Playlists = new List<Playlist>();

    protected override void OnInitialized()
    {
        Artist peter = new Artist();
        peter.Name = "Peter Němec";
        peter.Photo = "data:image/jpeg;base64,";

        Artist future = new Artist();
        future.Name = "Future";
        future.Photo = "https://gossipgist.com/uploads/2172/future-rapper.png";

        User u1 = new User();
        u1.Name = "Kopecek";

        User u2 = new User();
        u2.Name = "Pišta";

        Songs = new List<Song>()
        {
            new Song(){ Title = "Nová Éra", Photo = "https://i.ytimg.com/vi/o_0lVG0Vx2k/maxresdefault.jpg", Artist = peter, UserUploaded = u1 },
            new Song(){ Title = "Mask Off", Photo = "https://i.ytimg.com/vi/xvZqHgFz51I/maxresdefault.jpg", Artist = future, UserUploaded = u2 }
        };

        Artists = new List<Artist>();
        Artists.Add(peter);
        Artists.Add(future);

        Playlist playlist1 = new Playlist();
        playlist1.Name = "Best playlist";
        playlist1.Photo = "data:image/jpeg;base64,";
        playlist1.UserBy = u1;

        Playlist playlist2 = new Playlist();
        playlist2.Name = "Worst playlist";
        playlist2.Photo = "data:image/jpeg;base64,;";
        playlist2.UserBy = u2;

        Playlists.Add(playlist1);
        Playlists.Add(playlist2);

    }
}